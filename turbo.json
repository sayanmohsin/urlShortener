{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "generate": {
      "outputs": ["packages/db/prisma/client"],
      "cache": true
    },
    "push": {
      "cache": false
    }
  }
}
